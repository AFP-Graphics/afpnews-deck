{
  "version": 2,
  "name": "afpnews-deck",
  "builds": [
    {
      "src": "auth/index.js",
      "use": "@now/node"
    },
    {
      "src": "package.json",
      "use": "@now/static-build"
    }
  ],
  "routes": [
    {
      "src": "^/auth",
      "dest": "auth/index.js"
    },
    {
      "src": "^/sw.min.js",
      "headers": {
        "cache-control": "s-maxage=0",
        "X-XSS-Protection": "1; mode=block",
        "X-Content-Type-Options": "nosniff"
      },
      "dest": "/sw.min.js"
    },
    {
      "src": "^(.*)",
      "headers": {
        "X-XSS-Protection": "1; mode=block",
        "X-Content-Type-Options": "nosniff",
        "Vary": "Accept-Encoding",
        "X-Frame-Options": "DENY",
        "X-UA-Compatible": "IE=edge,chrome=1"
      },
      "dest": "/$1"
    }
  ],
  "env": {
    "AFPNEWS_DECK_AUTHORIZATION": "@afpnews-deck-authorization"
  }
}
